journal = {Urbe},
keywords = {City planning,Compromise programming,Decision-making,Geographic information systems,Sustainable development},
pages = {1--18},
title = {{Sustainability of the urban devevelopment of Campinas-SP: A multicriteria approach}},
volume = {12},
