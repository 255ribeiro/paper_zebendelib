file = {:C\:/Users/ferra/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Steele, Gabriel - 1993 - The Evolution of Lisp.pdf:pdf},
isbn = {0-89791-570-4},
issn = {03621340},
number = {3},
pages = {231--270},
publisher = {ACM},
title = {{The Evolution of Lisp}},
url = {http://doi.acm.org/10.1145/234286.1057818},
volume = {28},
year = {1993}
}
