file = {:G\:/Meu Drive/mendeleyBackup/Scalable_Learning_With_a_Structural_Recurrent_Neural_Network_for_Short-Term_Traffic_Prediction.pdf:pdf},
issn = {15581748},
journal = {IEEE Sensors Journal},
keywords = {Graph theory,intelligent transportation systems,machine learning,scalability,time series analysis},
number = {23},
pages = {11359--11366},
publisher = {IEEE},
title = {{Scalable Learning with a Structural Recurrent Neural Network for Short-Term Traffic Prediction}},
volume = {19},
year = {2019}
}
