keywords = {Aerial stereo imagery,CNN,Critical infrastructure,Power lines,Sparse representation,Threat estimation},
number = {March},
pages = {120762},
publisher = {Elsevier Inc.},
title = {{Fusion of CNN and sparse representation for threat estimation near power lines and poles infrastructure using aerial stereo imagery}},
url = {https://doi.org/10.1016/j.techfore.2021.120762},
volume = {168},
year = {2021}
}
