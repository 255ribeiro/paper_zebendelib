keywords = {Community,Digital divide,Land uses,Neighborhood,Smart cities,Urban Fabric},
number = {April 2019},
pages = {102978},
publisher = {Elsevier Ltd},
title = {{The urban digital lifestyle: An analytical framework for placing digital practices in a spatial context and for developing applicable policy}},
url = {https://doi.org/10.1016/j.cities.2020.102978},
volume = {111},
year = {2021}
}
