doi = {10.1680/jmuen.20.00012},
file = {:G\:/Meu Drive/mendeleyBackup/Using-vehicle-data-as-a-surrogate-for-highway-accident-dataProceedings-of-the-Institution-of-Civil-Engineers-Municipal-Engineer.pdf:pdf},
