journal = {ISPRS International Journal of Geo-Information},
keywords = {3d city model,aerial imagery,dsm,dtm,visualization},
number = {9},
pages = {339},
title = {{Semi-Automatic 3D City Model Generation from Large-Format Aerial Images}},
url = {http://www.mdpi.com/2220-9964/7/9/339},
